generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tb_event {
  id           Int    @id @default(autoincrement())
  objet_id     Int
  channel_id   String
  channel_name String
  event_name   String
  event_origin String
  event_time   String
  event_type   String
  rule_id      String
  rule_name    String
  is_send_api  String
  is_send_csv  String
  velocity     Float
  position_x   Float
  position_y   Float
  token        String
  cnpj         String
  nome_matriz  String
  filial       String
}
